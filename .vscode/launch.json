    "version": "0.2.0",
    "configurations": [
        {
            "name": "trading_demo",
            "request": "launch",
            "type": "dart"
        },
        {
            "name": "trading_demo (profile mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile"
        },
        {
            "name": "trading_demo (release mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release"
        },
        {
            "name": "Flutter on x86 Emulator",
            "request": "launch",
            "type": "dart",
            "program": "F:/src/flutter/bin",  // Replace with your Flutter installation path
            "args": ["-d", "emulator", "--target-arch=x86"]  // Launch emulator, specify x86 arch
          }
    ]
}